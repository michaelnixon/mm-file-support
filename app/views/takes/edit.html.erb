<div class="row wrapper white-bg page-heading">
  <div class="col-lg-10">
    <h2>Edit Take</h2>
    <ol class="breadcrumb">
      <li>
        <a href="<%= url_for :controller => 'projects' %>">Home</a>
      </li>
      <li class="active">
        <strong>Editing take</strong>
      </li>
    </ol>
  </div>
  <div class="col-lg-2">
  </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-title">
          <p class="p-gray"><b>Edit an existing take</b></p>
        </div>
        <div class="col-lg-12 border-bottom-mg-bt-top">
        </div>
        <div class="ibox-content">
          <%= render 'form' %>

          <%= link_to 'Show', @take %> |
          <%= link_to 'Back', takes_path %>
        </div>
      </div>
    </div>
  </div>
</div>



