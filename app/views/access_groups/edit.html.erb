<div class="row wrapper white-bg page-heading">
  <div class="col-lg-10">
    <h2>Edit Access Group</h2>
    <ol class="breadcrumb">
      <li>
        <a href="<%= url_for :controller => 'projects' %>">Home</a>
      </li>
      <li class="active">
        <strong>Edit Access Group</strong>
      </li>
    </ol>
  </div>
  <div class="col-lg-2">
  </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-title">
          <p class="p-gray"><b>Edit an existing access group</b></p>
        </div>
         <div class="col-lg-12 border-bottom-mg-bt-top">
         </div>
        <div class="ibox-content">
          <%= render 'form' %>
          <%= link_to back_link_html("Return to details of access group"), access_group_path(@access_group) %><br/>
          <%= link_to back_link_html("Return to list of access groups"), access_groups_path %>
        </div>
      </div>
    </div>
  </div>
</div>








